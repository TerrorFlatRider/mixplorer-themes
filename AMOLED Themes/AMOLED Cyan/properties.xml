<?xml version="1.0" encoding="utf-8"?>
<properties>

	<!-- MiXplorer Version: 6.28.x -->
	<!-- Theme Version: 1.6 -->
	<!-- https://terrorflatrider.com -->
	
	<entry key="title">AMOLED Cyan</entry>
	<entry key="author">TerrorFlatRider</entry>
	
	<!-- General -->
	<entry key="density">320</entry>
	<entry key="light_status_bar">false</entry> <!-- true for dark status bar icons -->
	<entry key="thumb_size">76</entry> <!-- dp -->
	<entry key="thumb_quality">60</entry> <!-- Percent -->
	<entry key="wallpaper">true</entry>
	<entry key="translucent_system_bars">true</entry>
	<entry key="background_dim">100</entry> <!-- (0~100) -->
	<entry key="uppercase_buttons">true</entry>
	<entry key="menu_overlap_anchor">false</entry>
	<!-- ICS ? false : true <entry key="square_icons">false</entry>-->

	<!-- Fonts -->
	<!--
	<entry key="font_primary"></entry>
	<entry key="font_secondary"></entry>
	<entry key="font_title"></entry>
	<entry key="font_popup"></entry>
	<entry key="font_editor"></entry>
	<entry key="font_hex"></entry>
	-->
  	
